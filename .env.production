# Production Environment Configuration
# IAA Feedback System

# Build Configuration
NODE_ENV=production
GENERATE_SOURCEMAP=false

# API Configuration
REACT_APP_API_BASE_URL=https://your-backend-url.com
REACT_APP_ENVIRONMENT=production

# Performance Optimizations
REACT_APP_ENABLE_CONSOLE_LOGS=false
REACT_APP_ENABLE_DEMO_DATA=false
REACT_APP_CACHE_DURATION=300000

# Security
REACT_APP_SECURE_COOKIES=true
REACT_APP_HTTPS_ONLY=true

# Analytics (if needed)
REACT_APP_ANALYTICS_ID=

# Feature Flags
REACT_APP_ENABLE_ANALYTICS_TAB=false
REACT_APP_ENABLE_DEMO_NOTIFICATIONS=false
REACT_APP_ENABLE_RESET_SYSTEM=false
REACT_APP_ENABLE_ADMIN_REGISTRATION=false
